<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/boardprerences">
        <CheckBoxPreference android:title="@string/pref_sound" android:key="moveSounds" android:summary="@string/pref_sound_desc" android:defaultValue="false" />
